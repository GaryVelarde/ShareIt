
@{
    Layout = null;
    string fecha = ViewBag.FechaActual;
    string[] fechas = fecha.Split('/');
    if(fechas[0].ToString().Length == 1)
    {
        fechas[0] = "0" + fechas[0];
    }
    string dia = fechas[0];
    string mes = fechas[1];
    string anio = fechas[2];

    string fechaC = anio + "-" + mes + "-" + dia;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>ShareIt - Registrate</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="~/Resources/Login/images/icons/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="~/Resources/Login/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <link href="~/Resources/css/fontawesome-free/css/all.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Resources/Login/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Resources/Login/css/main.css">
    <script src="~/Resources/js/jquery-3.6.0.min.js"></script>
    <script src="~/Resources/js/Login.js"></script>
    <style>
        input[type=number]::-webkit-inner-spin-button,
        input[type=number]::-webkit-outer-spin-button {
            -webkit-appearance: none;
            margin: 0;
        }

        input[type=number] {
            -moz-appearance: textfield;
        }
    </style>
</head>
<body style="background-color: #666666;">
    <input type="hidden" id="ipFechaActual" value="@fechaC" />
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="login100-form validate-form" style="padding: 50px 55px 55px 55px;">
                    <span class="login100-form-title p-b-43">
                        Registrarte es rápido y sencillo
                    </span>

                    <div id="validacionipNombres" class="wrap-input100 validate-input" data-validate="Por favor ingrese sus nombres.">
                        <input id="ipNombres" class="input100" type="text">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Nombres</span>
                    </div>

                    <div id="validacionipApellidos" class="wrap-input100 validate-input" data-validate="Por favor ingrese sus apellidos.">
                        <input id="ipApellidos" class="input100" type="text">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Apellidos</span>
                    </div>

                    <div id="validacionipCelular" class="wrap-input100 validate-input" data-validate="Por favor ingrese su nro celular.">
                        <input id="ipCelular" class="input100" type="number">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Nro Celular</span>
                    </div>

                    <div id="validacionipFechaNacimiento" class="wrap-input100 validate-input" data-validate="Por favor ingrese su fecha de nacimiento.">
                        <input id="ipFechaNacimiento" class="input100 has-val" type="date" value="@fechaC">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Fecha de nacimiento</span>
                    </div>

                    <div id="validacionIpEmail" class="wrap-input100 validate-input" data-validate="El correo no es válido.">
                        <input id="ipCorreo" class="input100" type="text" name="email" onkeyup="validarEmail($('#ipCorreo').val());">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Email</span>
                    </div>

                    <div id="validacionIpPassword" class="wrap-input100 validate-input" data-validate="Se necesita una contraseña segura.">
                        <input id="ipPassword" class="input100" style="width: 90%;" type="password" name="pass">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Password</span>
                        <button style="height: 100%;"><i class="fas fa-eye" onclick="visualizarClave('ipPassword',this);"></i></button>
                    </div>

                    <div id="validacionIpPassword2" class="wrap-input100 validate-input" data-validate="Las contraseñas no coinciden.">
                        <input id="ipPassword2" class="input100" style="width: 90%;" type="password" name="pass">
                        <span class="focus-input100"></span>
                        <span class="label-input100">Repita su password</span>
                        <button style="height: 100%;"><i class="fas fa-eye" onclick="visualizarClave('ipPassword2',this);"></i></button>
                    </div>

                    <div class="flex-sb-m w-full p-t-3 p-b-32">
                        <div class="contact100-form-checkbox">
                            <input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
                            <label class="label-checkbox100" for="ckb1">
                                Acepto los <a href="#" style="text-decoration: underline;">términos y condiciones</a>
                            </label>
                        </div>

                    </div>


                    <div class="container-login100-form-btn">
                        <button id="btnLogin" onclick="validacionDatos();" class="login100-form-btn">
                            Registrarse ahora
                        </button>
                    </div>
                    <div style="height: 10px;"></div>
                    <div class="container-login100-form-btn">
                        <button id="btnLogin" onclick="window.location.href = 'Login';" class="login100-form-btn">
                            Ya tengo una cuenta
                        </button>
                    </div>

                </div>

                <div class="login100-more" style="background-image: url('/Resources/Login/images/bg-01.jpg');">
                </div>
            </div>
        </div>
    </div>




    <!--===============================================================================================-->
    <script src="~/Resources/Login/js/main.js"></script>

</body>
</html>